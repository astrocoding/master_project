    \node [block, below left = of sf] (tf) {create time filter};
    \node [block, below = 1.5cm of sf] (cb) {create background};
    \node [block] (cp) at (tf|-cb) {cut pcu change};
    \node [block] (fc) at (gx|-cb){find channels};
    \node [block, below = of cb] (el) {extract lightcurve};
    \node [block, below = of el] (rb) {rebin background};
    \node [block, below = of rb] (ab) {account for background};
    \node [block, below = of ab] (cx) {cut x-ray flares};
    \node [block, below = of cx] (cps) {create power spectrum};
    \node [block, below = of cps] (cpc) {create power colours};

    \path [line] (fd) to (sf);
    \path [line, bend right] (sf.west) to (tf.north);
    \path [line, bend left] (sf.east) to (gx.north);
    \path [line, bend left=30] (tf.east) to (cb.north);
    \path [line] (gx) to (fc);
    \path [line, bend left] (fc.south) to (el.east);
    \path [line, bend right] (cp.south) to (el.west);
    \path [line] (tf) to (cp);
    \path [line] (cb) to (el);
    \path [line] (el) to (rb);
    \path [line] (rb) to (ab);
    \path [line] (ab) to (cx);
    \path [line] (cx) to (cps);
    \path [line] (cps) to (cpc);

